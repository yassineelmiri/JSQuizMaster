    <main class="d-flex flex-nowrap">
      

      <section class="w-100">
      

        <form action="/submit-test" method="POST" class="mx-0 row gx-3 gy-4 mt-3">
          <div class="col-md-6">
            <label for="inputName" class="form-label">Name:</label>
            <input
              placeholder="Test Name"
              type="text"
              class="form-control"
              id="inputName"
              name="Name"
              required
            />
          </div>
          
          <div class="col-md-6">
            <label for="inputDate" class="form-label">Test Date:</label>
            <input
              type="date"
              class="form-control"
              id="inputDate"
              name="testDate"
            />
          </div>
        
          <div class="col-md-12">
            <label for="inputDescription" class="form-label">Description:</label>
            <input
              type="text"
              class="form-control"
              id="inputDescription"
              name="Description"
            />
          </div>
        
          <div class="col-md-6">
            <label for="inputProfessor" class="form-label">Professor ID:</label>
            <input
              type="number"
              class="form-control"
              id="inputProfessor"
              name="professor_id"
              required
            />
          </div>
        
          <div class="col-12">
            <button type="submit" class="btn btn-primary">Submit</button>
          </div>
        </form>
      </section>
    </main>
    
 
